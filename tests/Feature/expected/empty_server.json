                                    "$ref": "#\/components\/schemas\/jsonapi-version"
                                }
                            }
                        }
                    }
                }
            },
            "404": {
                "description": "Not Found",
                "content": {
                    "application\/vnd.api+json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "$ref": "#\/components\/schemas\/error"
                                },
                                "jsonapi-version": {
                                    "$ref": "#\/components\/schemas\/jsonapi-version"
                                }
                            }
                        }
                    }
                }
            },
            "422": {
                "description": "Unprocessable Entity",
                "content": {
                    "application\/vnd.api+json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "$ref": "#\/components\/schemas\/error"
                                },
                                "jsonapi-version": {
                                    "$ref": "#\/components\/schemas\/jsonapi-version"
                                }
                            }

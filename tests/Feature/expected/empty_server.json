{
    "openapi": "3.0.2",
    "info": {
        "title": "API",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": ""
        }
    ],
    "paths": {
        "\/": {
            "description": "No paths configured yet"
        }
    },
    "components": {
        "schemas": {
            "jsonapi-version": {
                "title": "JSON:API version",
                "type": "object",
                "properties": {
                    "version": {
                        "title": "version",
                        "type": "string",
                        "example": "1.0"
                    }
                }
            },
            "error": {
                "title": "Error",
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "detail": {
                            "type": "string"
                        },
                        "status": {
                            "type": "string"
                        },
                        "title": {
                            "type": "string"
                        },
                        "source": {
                            "type": "object",
                            "properties": {
                                "pointer": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "required": [
                    "title",
                    "status"
                ]
            }
        },
        "responses": {
            "400": {
                "description": "Bad Request",
                "content": {
                    "application\/vnd.api+json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "error": {
                                    "$ref": "#\/components\/schemas\/error"
                                },
                                "jsonapi-version": {
                                    "$ref": "#\/components\/schemas\/jsonapi-version"
                                }
                            }
                        }
                    }
                }
            },
            "401": {
                "description": "Forbidden",
                "content": {
                    "application\/vnd.api+json": {
